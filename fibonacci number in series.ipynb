   "source": [
    "def fibonacci(n):\n",
    "    if n<=1:\n",
    "        return n\n",
    "    return fibonacci(n-1)+fibonacci(n-2)\n",
    "n=int(input(\"enter n:\"))\n",
    "print(f\"{n}th term of the fibonacci series is:\",fibonacci(n))"
   ]
