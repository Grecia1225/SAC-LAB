    "import sys\n",
    "import math\n",
    "s1=float(input(\"enter side 1:\"))\n",
    "s2=float(input(\"enter side 2:\"))\n",
    "s3=float(input(\"enter side 3:\"))\n",
    "s=(s1+s2+s3)/2\n",
    "area=math.sqrt(s*(s-s1)*(s-s2)*(s-s3))\n",
    "print(f\"Area of triangle is:{area:.2f}\")"
