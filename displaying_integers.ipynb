    "def even_digit_sum(n):\n",
    "    digit_sum=sum(int(d) for d in str(n))\n",
    "    return digit_sum%2==0\n",
    "def display(start, end):\n",
    "    for num in range(start, end+1):\n",
    "        if even_digit_sum(num):\n",
    "            print(num,end=\" \")\n",
    "display(100, 200)"
   ]
