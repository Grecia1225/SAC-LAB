   "source": [
    "s = input(\"Enter a string:\")\n",
    "rev = \"\"\n",
    "\n",
    "for ch in s:\n",
    "    rev = ch + rev\n",
    "\n",
    "if s == rev:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")"
   ]
